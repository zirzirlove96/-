    "c=int(input())\n",
    "\n",
    "a+=c//60 #시간은 몫을 더한것\n",
    "b+=c%60 #분은 나머지를 더한 것\n",
    "\n",
    "if b>=60:\n",
    "    a+=1\n",
    "    b-=60\n",
    "if a>=24:#24시는 0시로 표현해야 함.\n",
    "    a-=24\n",
    "\n",
    "print(a,b)\n"
