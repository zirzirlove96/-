    "\n",
    "n=int(input())\n",
    "s=[-1]+list(map(int, input().split()))\n",
    "line=[]\n",
    "\n",
    "for i in range(n,0,-1):\n",
    "    line.insert(s[i],i)\n",
    "\n",
    "print(line)\n"
