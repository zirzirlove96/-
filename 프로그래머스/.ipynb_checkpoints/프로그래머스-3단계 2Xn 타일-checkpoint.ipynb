    "def solution(n):\n",
    "    answer = 0\n",
    "    a=[0 for _ in range(n+1)]\n",
    "    a[0]=1\n",
    "    a[1]=1\n",
    "\n",
    "    for i in range(2,n+1):\n",
    "        a[i]=(a[i-1]+a[i-2])%1000000007\n",
    "        \n",
    "    answer=a[n]\n",
    "    return answer\n",
    "\n",
    "\n",
    "print(solution(5))\n"
   ]
