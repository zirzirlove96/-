    "def solution(n):\n",
    "    answer = []\n",
    "\n",
    "    n=str(n)\n",
    "    n=n[::-1]\n",
    "    for i in n:\n",
    "        j=int(i)\n",
    "        answer.append(j)\n",
    "    return answer\n",
    "\n",
    "print(solution(12345))"
   ]
